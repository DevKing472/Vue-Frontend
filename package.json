{
  "name": "vue-storefront",
  "description": "A Vue.js, PWA eCommerce frontend",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": ">=8.x <10.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DivanteLtd/vue-storefront"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "webpack",
    "starter",
    "server-side",
    "boilerplate"
  ],
  "author": "pkarw <pkarwatka@divante.pl> and contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DivanteLtd/vue-storefront/issues"
  },
  "homepage": "https://github.com/DivanteLtd/vue-storefront/README.md",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./core/scripts/server",
    "installer": "node ./core/scripts/installer",
    "installer:ci": "yarn installer --default-config",
    "all": "node ./core/scripts/all",
    "dev": "node ./core/scripts/server",
    "dev:inspect": "node --inspect ./core/scripts/server",
    "build:client": "cross-env NODE_ENV=production webpack --config ./core/build/webpack.prod.client.config.js --mode production --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config ./core/build/webpack.prod.server.config.js --mode production --progress --hide-modules",
    "build": "rimraf dist && yarn build:client && yarn build:server",
    "unit": "karma start ./test/unit/karma.conf.js --single-run",
    "tests:e2e": "cypress open",
    "tests:e2e:ci": "cypress run",
    "lint": "eslint --ext .js,.vue core src",
    "lerna": "lerna"
  },
  "dependencies": {
    "ajv": "^5.2.2",
    "app-root-path": "^2.0.1",
    "bodybuilder": "^2.2.1",
    "command-exists": "^1.2.2",
    "commander": "^2.15.1",
    "config": "^1.30.0",
    "cross-env": "^3.1.4",
    "d3-dsv": "^1.0.8",
    "detect-installed": "^2.0.4",
    "elasticsearch": "^13.3.1",
    "empty-dir": "^0.2.1",
    "es6-promise": "^4.2.4",
    "express": "^4.14.0",
    "fs-exists-sync": "^0.1.0",
    "he": "^1.1.1",
    "inquirer": "^3.3.0",
    "is-windows": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jsonfile": "^4.0.0",
    "localforage": "^1.5.0",
    "lru-cache": "^4.0.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.21.0",
    "npm": "^5.7.1",
    "object-hash": "^1.2.0",
    "object-sizeof": "^1.2.0",
    "print-message": "^2.1.0",
    "scrollreveal": "^3.3.6",
    "url-parse": "^1.2.0",
    "vue": "^2.5.2",
    "vue-analytics": "^5.8.0",
    "vue-carousel": "^0.6.9",
    "vue-i18n": "^7.4.2",
    "vue-lazyload": "^1.2.3",
    "vue-meta": "^1.4.3",
    "vue-no-ssr": "^0.2.2",
    "vue-observe-visibility": "^0.3.1",
    "vue-offline": "^1.0.7",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.2",
    "vsf-payment-stripe": "^1.0.0",
    "vue2-google-maps": "^0.8.4",
    "vue2-slugify": "0.0.1",
    "vuelidate": "^0.6.2",
    "vuex": "^3.0.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.x",
    "babel-preset-stage-2": "^6.13.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chai": "^3.5.0",
    "css-loader": "^0.28.11",
    "cypress": "^2.1.0",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "eslint-plugin-vue-storefront": "^0.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^1.1.2",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon-chai": "^1.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^3.0.0",
    "lerna": "^2.9.0",
    "markdown-to-vue-loader": "^0.4.0",
    "mocha": "^3.0.2",
    "node-sass": "^4.9.0",
    "phantomjs-prebuilt": "^2.1.10",
    "rimraf": "^2.6.0",
    "sass-loader": "^7.0.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "shelljs": "^0.8.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.0.1",
    "val-loader": "^1.1.0",
    "vue-eslint-parser": "^2.0.3",
    "vue-loader": "^15.1.0",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.2"
  },
  "workspaces": [
    "core/store",
    "src/extensions/*",
    "src/themes/*"
  ]
}
